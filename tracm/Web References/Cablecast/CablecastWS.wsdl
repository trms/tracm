      <wsdl:output message="tns:UpdateShowRecordSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="CreateNewProducer">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Creates a new Producer at a particular Location.  Returns the ProducerID of the newly created producer.</wsdl:documentation>
