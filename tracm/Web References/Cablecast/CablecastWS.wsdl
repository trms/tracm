      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Adds a new Reel to a show</wsdl:documentation>
      <wsdl:input message="tns:AddReelSoapIn" />
      <wsdl:output message="tns:AddReelSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="UpdateReel">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Updates the given Reel.  Reel must belong to ShowID provided.  All feilds are required.  Blank or empty fields will overwrite existing reel data.</wsdl:documentation>
      <wsdl:input message="tns:UpdateReelSoapIn" />
      <wsdl:output message="tns:UpdateReelSoapOut" />
    </wsdl:operation>
